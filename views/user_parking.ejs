    <div class="pk-part2">
      <nav>
        <div class="pk-profile-part1">
          <div style="width: 3rem; height: 3rem; border-radius: 9999px; background-color: #fff; overflow: hidden;">
            <img style="width: 100%; height: 100%; object-fit: cover;" src="/Images/Uploads/<%= user.profile_pic %>" alt="">
          </div>
          <div class="pk-profile-part1-name"><%= user.name %></div>
        </div>
      </nav>

      <div class="pk-heading-part">Select date and time for Booking</div>

      <div class="pk-searching-part">
        <input type="date" class="pk-searchbar" />
        <input type="time" class="pk-searchbar" />
        <button class="pk-searchbutton">Search</button>
      </div>

      <table class="pk-services-table">
        <tr>
          <td class="pk-table-selected">
            <img
              style="height: 50px; width: 85px"
              src="/Images/car_upscaled.png"
              alt=""
            />
